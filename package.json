{
  "name": "@turbo-eth/core-wagmi",
  "version": "0.0.0-beta.8",
  "license": "MIT",
  "author": "Kames Geraghty",
  "source": "src/index.tsx",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.module.js"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "bin": {
    "commitlint": "./node_modules/@commitlint/cli/lib/cli.js"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "microbundle --jsx React.createElement",
    "start": "microbundle --jsx React.createElement watch",
    "lint": "dts lint",
    "prepare": "husky install ; microbundle",
    "size": "size-limit",
    "test": "dts test --passWithNoTests",
    "docs": "yarn typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true --namedAnchors true --entryDocument index.md",
    "docs:html": "yarn typedoc --plugin none --out docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint",
      "commit-msg": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/index.cjs.production.min.js",
      "limit": "32 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "32 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/create-react-app": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "dts-cli": "^1.6.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "microbundle": "^0.15.1",
    "react": ">=17",
    "react-dom": ">=17",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.8",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "clsx": "^1.2.1",
    "prop-types": "15.8.1",
    "viem": "^0.3.19",
    "wagmi": "1.0.1"
  }
}
